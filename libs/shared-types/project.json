{
  "sourceRoot": "libs/shared-types/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/core-logic",
        "main": "libs/core-logic/src/index.ts",
        "tsConfig": "libs/core-logic/tsconfig.lib.json",
        "assets": ["libs/core-logic/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/shared-types/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/shared-types"],
      "options": {
        "jestConfig": "libs/shared-types/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
